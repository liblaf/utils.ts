    "allowCompoundWords": true,
    "ignorePaths": [
        "**/.cspell.*",
        "**/.git",
        "**/.vscode",
        "**/*-lock.*",
        "**/*.lock*"
    ],
    "language": "en",
    "version": "0.2",
    "words": [
        "devskim",
        "dotenv",
        "jscoverage",
        "jspm",
        "liblaf",
        "nuxt",
        "pids",
        "pyrightconfig",
        "stefanzweifel",
        "vuepress",
        "wscript"
    ]
}